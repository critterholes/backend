{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "moduleResolution": "node",
    "lib": ["es2020"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": ".vercel/output",
    "types": ["node", "@vercel/node"]
  },
  "include": ["api/**/*.ts"],
  "exclude": ["node_modules"]
}
